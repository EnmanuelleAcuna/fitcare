<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Sistema de Control de Gimnasios" />
    <meta name="author" content="Enmanuelle Acuña & Jose Alexander González" />
    <title>@ViewData["Title"] - fitcare</title>

    <link rel="stylesheet" href="~/lib/nice/dist/css/style.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.css" />
    <link rel="stylesheet" href="~/lib/datatables/css/5_buttons.dataTables.min.css" />
</head>

<body>
    <partial name="_PreloaderAdminPartial" />

    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper" data-navbarbg="skin6" data-theme="light" data-layout="vertical" data-sidebartype="full" data-boxed-layout="full">
        <partial name="_HeaderAdminPartial" />
        <partial name="_SidebarAdminPartial" />

        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            @RenderBody()
        </div>
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->

        <partial name="_FooterAdminPartial" />
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->

    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="~/lib/nice/assets/extra-libs/sparkline/sparkline.js"></script>
    <script type="text/javascript" src="~/lib/nice/dist/js/waves.js"></script>
    <script type="text/javascript" src="~/lib/nice/dist/js/sidebarmenu.js"></script>
    <script type="text/javascript" src="~/lib/nice/dist/js/custom.js"></script>
    @* <script type="text/javascript" src="~/js/app.js"></script> *@

    <script type="text/javascript" src="~/lib/datatables/js/dataTables.dataTables.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>

    <script type="text/javascript" src="~/lib/datatables/js/1_dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/2_pdfmake.min.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/3_vfs_fonts.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/4_buttons.html5.min.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/6_jszip.min.js"></script>
    <script type="text/javascript" src="~/lib/datatables/js/7_buttons.print.min.js"></script>

    @RenderSection("Scripts", required: false)

    @* @Scripts.Render("~/bundles/popper") No borrar aun has probar bootstrap *@
</body>

</html>
